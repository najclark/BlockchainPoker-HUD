{
    "manifest_version": 2,
    "name": "Blockchain Poker Slave",
    "version": "0.1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Run Blockchain Poker Slave"
    },
    "background": {
       "scripts": [
         "background.js"
       ]
     },
    "content_scripts": [
        {
            "matches": ["https://blockchain.poker/"],
            "js": ["scripts.js"]
        }
    ]
}
